alias ls='ls -G'
alias la='ls -la -G'
alias ll='ls -l -G'
alias ld='ls -d *(/)'
alias h='history'
alias r='fc -s'
alias cdiff=colordiff
alias cgrep='grep --color=auto'
alias egrep='grep -E'
alias psgrep='ps -ef | grep --color'

alias vi=vim
alias view='vim -R'

alias xarg1='xargs -n1 -i'

